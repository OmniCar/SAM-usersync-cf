{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-immutable"
  ],
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "no-unused-expression": {
      "severity": "warning"
    },
    "no-unused-variable": {
      "severity": "warning"
    }
  }
}
